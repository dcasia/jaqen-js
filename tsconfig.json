{
    "compilerOptions": {
        "target": "ES2015",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "declaration": true,
        "removeComments": true,
        "outDir": "distribution",
        "lib": [
            "DOM",
            "ES5"
        ]
    }
}
